<Ui>
	<Actor name="EMS_ActorTemplate" virtual="true" mixin="EMS_ActorMixin">
		<Scripts>
			<OnLoad>        if self.OnLoad         then self:OnLoad()          end</OnLoad>
			<OnUpdate>      if self.OnUpdate       then self:OnUpdate(elapsed) end</OnUpdate>
			<OnModelLoading>if self.OnModelLoading then self:OnModelLoading()  end</OnModelLoading>
			<OnModelLoaded> if self.OnModelLoaded  then self:OnModelLoaded()   end</OnModelLoaded>
			<OnAnimFinished>if self.OnAnimFinished then self:OnAnimFinished()  end</OnAnimFinished>
		</Scripts>
	</Actor>
</Ui>
